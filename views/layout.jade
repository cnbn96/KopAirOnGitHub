    body.menubar-hoverable.header-fixed
        // BEGIN HEADER
        header#header
            .headerbar
                // Brand and toggle get grouped for better mobile display
                .headerbar-left
                    ul.header-nav.header-nav-options
                        li.header-nav-brand
                            .brand-holder
                                a(href='./html/dashboards/dashboard.html')
                                    span.text-lg.text-bold.text-primary KOP AIR
                        li
                            a.btn.btn-icon-toggle.menubar-toggle(data-toggle='menubar', href='javascript:void(0);')
                                i.fa.fa-bars
                // Collect the nav links, forms, and other content for toggling
                .headerbar-right
                    ul.header-nav.header-nav-profile
                        li.dropdown
                            a.dropdown-toggle.ink-reaction(href='javascript:void(0);', data-toggle='dropdown')
                                img(src='/assets/img/avatar1.jpg?1403934956', alt='')
                                span.profile-info
                                    | Daniel Johnson
                                    small Administrator
                            ul.dropdown-menu.animation-dock
                                li
                                    a(href='./html/pages/login.html')
                                        i.fa.fa-fw.fa-power-off.text-danger
                                        |  Logout
                            // end .dropdown-menu
                        // end .dropdown
                    // end .header-nav-profile
                // end #header-navbar-collapse
        // END HEADER
        // BEGIN BASE
        #base
            // BEGIN OFFCANVAS LEFT
            .offcanvas
            // end .offcanvas
            // END OFFCANVAS LEFT
            // BEGIN CONTENT
            #content
                section
                    .section-body
                        block content
                // END CONTENT
                // BEGIN MENUBAR
                #menubar.menubar-inverse
                    .menubar-fixed-panel
                        div
                            a.btn.btn-icon-toggle.btn-default.menubar-toggle(data-toggle='menubar', href='javascript:void(0);')
                                i.fa.fa-bars
                        .expanded
                            a(href='./dashboard.html')
                                span.text-lg.text-bold.text-primary MATERIAL ADMIN
                    .menubar-scroll-panel
                        // BEGIN MAIN MENU
                        ul#main-menu.gui-controls
                            // BEGIN DASHBOARD
                            li
                                a(href='./dashboard.html')
                                    .gui-icon
                                        i.md.md-home
                                    span.title Dashboard
                            // end /menu-li
                            // END DASHBOARD
                        // end .main-menu
                        // END MAIN MENU
                        .menubar-foot-panel
                            small.no-linebreak.hidden-folded
                                span.opacity-75 Copyright © 2016
                                strong Backerlite
                    // end .menubar-scroll-panel
                // end #menubar
                // END MENUBAR
            // end #base
            // END BASE
            // BEGIN JAVASCRIPT
            script(src='./assets/js/libs/jquery/jquery-1.11.2.min.js')
            script(src='./assets/js/libs/jquery/jquery-migrate-1.2.1.min.js')
            script(src='./assets/js/libs/bootstrap/bootstrap.min.js')
            script(src='./assets/js/libs/spin.js/spin.min.js')
            script(src='./assets/js/libs/autosize/jquery.autosize.min.js')                                 
            // END JAVASCRIPT

            block js-library

            script(src='/assets/js/libs/nanoscroller/jquery.nanoscroller.min.js')
